@model IEnumerable<PFE_EMI.Models.ListePFEEtudiants>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                Etudiant
            </th>
            <th>
                sujet PFE
            </th>
           
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.nameEtudiant)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.nomPFE)
            </td>
            
            
            <td>
                <a asp-action="Details" class="btn btn-primary" asp-route-id="@item.IdEtudiant">Details</a>
                @*<a asp-action="Edit" asp-route-id="@item.id_remarque">Edit</a> |
        <a asp-action="Details" asp-route-id="@item.id_remarque">Details</a> |
        <a asp-action="Delete" asp-route-id="@item.id_remarque">Delete</a>*@
            </td>
        </tr>
}
    </tbody>
</table>
